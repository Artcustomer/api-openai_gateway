{% extends 'application/base_application.html.twig' %}

{% block title %}{{ app_name }} | {{ gatewayName }} - {{ 'main_menu.feature.audio_completion'|trans }}{% endblock %}

{% block applicationPageTitle %}{{ gatewayName }} - {{ 'main_menu.feature.audio_completion'|trans }}{% endblock %}

{% block applicationSection %}
    {% if errorMessage %}
        <div class="alert alert-dismissible alert-danger">
            <strong>{{ errorMessage }}</strong>
        </div>
    {% endif %}

    <div class="row">
        <div class="col-lg-12">
            <div class="card text-center">
                <div class="card-body">
                    {{ component('OpenAI:FeatureAudioCompletion') }}
                </div>
            </div>
        </div>
    </div>
{% endblock %}
